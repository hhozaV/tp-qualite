<Window x:Class="NoteBook.EditUnitsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoteBook"
        mc:Ignorable="d"
        Title="Edit Units" Height="450" Width="450">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Height="32" Margin="85,70,0,0" TextWrapping="Wrap" FontSize="16" Text="Courses Units" VerticalAlignment="Top" Width="151"/>
        <ListBox HorizontalAlignment="Left" FontSize="15" Height="207" Margin="85,102,0,0" VerticalAlignment="Top" Width="273" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Button Content="Add" Click="EditUE" HorizontalAlignment="Left" Height="35" Margin="85,343,0,0" VerticalAlignment="Top" Width="103"/>
        <Button Content="Remove" HorizontalAlignment="Left" Height="35" Margin="255,343,0,0" VerticalAlignment="Top" Width="103"/>

    </Grid>
</Window>
